{
  "extends": ["config:base", ":dependencyDashboard"],
  "hostRules": [
    {
      "matchHost": "npm.pkg.github.com",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "rebaseWhen": "behind-base-branch",
  "packageRules": [
    {
      "managers": ["npm"],
      "updateTypes": ["major"],
      "enabled": false
    },
    {
      "paths": ["npm/**", "yarn/**"],
      "additionalBranchPrefix": "{{baseDir}}-",
      "commitMessageSuffix": "({{baseDir}})"
    }
  ]
}
